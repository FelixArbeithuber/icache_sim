'a' {
    0x0100..32..0x0200
}

'b' {
    0x2100..32..0x2200
}

'c' {
    0x4100..32..0x4200
}

'd' {
    0x6100..32..0x6200
}

'e1' {
    0x8000..32..0x8100
}

'e2' {
    0x8000..32..0x8100
    0x8100..32..0x8104 // extra instruction
}

compare 'block-1' {
    loop(10000) {
        a()
        b()
        c()
        d()
        e1()
    }
}

compare 'block-2' {
    loop(10000) {
        a()
        b()
        c()
        d()
        e2()
    }
}
