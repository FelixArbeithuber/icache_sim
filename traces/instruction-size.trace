'a' {
    0x0100..32..0x0200
}

'b' {
    0x2100..32..0x2200
}

'c' {
    0x4100..32..0x4200
}

'd' {
    0x6100..32..0x6200
}

'e1' {
    0x8000..32..0x80FC
    0x80FC..16..0x80FE

    0x80FE..16..0x8100
}

'e2' {
    0x8000..32..0x80FC
    0x80FC..16..0x80FE

    0x80FE..32..0x8102 // large instruction
}

compare 'fits-cacheline' {
    loop(10000) {
        a()
        b()
        c()
        d()
        e1()
    }
}

compare 'doesnt-fit-cacheline' {
    loop(10000) {
        a()
        b()
        c()
        d()
        e2()
    }
}
