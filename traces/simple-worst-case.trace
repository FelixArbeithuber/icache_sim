// the total cache size is 32KB
// => 4 functions with 8KB of instructions completely fill the cache

fn a() {
    0x0..32..0x2000
}

fn b() {
    0x1F40..32..0x3E80
}

fn c() {
    0x3E80..32..0x5DC0
}

fn d() {
    0x5DC0..32..0x7D00
}

fn e() {
    0x5DC0..32..0x7D40
}

'block 1' {
    loop(10000) {
        a()
        b()
        c()
        d()
    }
}

'block 2' {
    loop(8000) {
        a()
        b()
        c()
        d()
        e()
    }
}
