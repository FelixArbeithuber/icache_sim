'a' {
    0x0..32..0x1F40
}

'b' {
    0x1F40..32..0x3E80
}

'c' {
    0x3E80..32..0x5DC0
}

compare 'functions' {
    loop(10000) {
        a()
        a()
        b()
        b()
        c()
    }
}

compare 'inlined' {
    loop(10000) {
        0x0..32..0x1F40   // inlined function a()
        0x1F40..32..0x3E80 // inlined function a()
        0x3E80..32..0x5DC0 // inlined function b()
        0x5DC0..32..0x7D00 // inlined function b()
        0x7D00..32..0x9C40 // inlined function c()
    }
}
