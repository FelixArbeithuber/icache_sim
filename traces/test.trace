// the total cache size is 32KB
// => 4 functions with 8KB of instructions completely fill the cache

fn a() {
    0x00..32..0x2000
}

fn b() {
    0x2000..32..0x4000
}

fn c() {
    0x4000..32..0x6000
}

fn d1() {
    0x6000..32..0x8000
}

fn d2() {
    0x6000..32..0x8000
    0x8000..32..0x8004
}

'block 1' {
    loop(2) {
        a()
        b()
        c()
        d1()
    }
}

'block 2' {
    loop(2) {
        a()
        b()
        c()
        d2()
    }
}
