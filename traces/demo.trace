// this is a comment

// this is a function definition
fn b_() {
    0x0..16..0x2 // access addresses [0x0, 0x2] because of 16b instruction size
    0x0..32..0x4 // access addresses [0x0] because of 32b instruction size
}

fn a_b() {
    0x0..32..0x4
}

fn c() {
    0x20..32..0x40
}

// all declared blocks get executed independently and compared
'block-_ name' {
    a_b() // function call

    // a switch statement
    switch:
        // the label is the probability weight of this case
        // in this case the branches are equally likely
        (1): {
            // the block can be anything
            loop(3) {
                0x1..16..0x3
            }
        }
        (1): {
            0x20..16..0x30
        }
    endswitch

    // a loop with a fixed iteration count
    loop (2) {
        0x20..32..0x40
    }
}
